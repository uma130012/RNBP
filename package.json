{
  "name": "RNBP",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ios-qa": "react-native run-ios --scheme 'RNBP-QA'",
    "ios-stage": "react-native run-ios --scheme 'RNBP-Stage'",
    "ios-prod": "react-native run-ios --scheme 'RNBP'",
    "android-stage": "react-native run-android --variant=stageDebug",
    "android-qa": "react-native run-android --variant=qaDebug",
    "android-prod": "react-native run-android --variant=prodDebug",
    "android-stage-release": "cd android && ./gradlew assembleStageRelease",
    "android-qa-release": "cd android && ./gradlew assembleQaRelease",
    "android-prod-release": "cd android && ./gradlew assembleProdRelease",
    "android-stage-bundle": "cd android && ./gradlew bundleStageRelease",
    "android-qa-bundle": "cd android && ./gradlew bundleQaRelease",
    "android-prod-bundle": "cd android && ./gradlew bundleProdRelease",
    "open-apk": "open ./android/app/build/outputs/apk/",
    "open-bundle": "open ./android/app/build/outputs/bundle/",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "kn": "killall node",
    "adb-r": "adb reverse tcp:8081 tcp:8081"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "@react-navigation/stack": "^6.3.25",
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "^1.6.7",
    "formik": "^2.4.5",
    "i18next": "^23.10.0",
    "react": "18.2.0",
    "react-i18next": "^14.0.5",
    "react-native": "0.72.10",
    "react-native-confirmation-code-field": "^7.4.0",
    "react-native-element-dropdown": "^2.10.2",
    "react-native-config": "^1.5.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keychain": "^8.2.0",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-toast-message": "^2.2.0",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0",
    "socket.io-client": "^4.7.4",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
